<script>
function changeFont(t){$(".multi-fontsize").css("font-size",t+"px"),$("#font_selection_box").addClass(t)}function setCookie(t,e,i){var n=new Date;n.setTime(n.getTime()+24*i*60*60*1e3),n="expires="+n.toGMTString(),document.cookie=t+"="+e+"; "+n}function getCookie(t){for(var e=t+"=",i=document.cookie.split(";"),n=0;n<i.length;n++){for(var s=i[n];" "==s.charAt(0);)s=s.substring(1);if(-1!=s.indexOf(e))return s.substring(e.length,s.length)}return""}$(document).ready(function(){document.querySelector(".toggleBtn").addEventListener("click",function(){this.classList.contains("active")?this.className="toggleBtn":this.className+=" active"});for(var t="",e=1;e<=3;e++)t+='<p id="fontsize_opt'+e+'" class="multi_font_icon"></p>';$("#font_selection_box").html(t),$("#fontsize_opt1").text("Medium"),$("#fontsize_opt2").text("Large"),$("#fontsize_opt3").text("Larger"),$(".multi_font_icon").click(function(t){$(this).removeClass("currentSize").addClass("currentSize");var e=16;switch((t=t.target.id).substr(t.length-1)){case"1":e=17,c=$(this).siblings(".multi_font_icon").removeClass("currentSize");break;case"2":e=19,c=$(this).siblings(".multi_font_icon").removeClass("currentSize");break;case"3":e=22,c=$(this).siblings(".multi_font_icon").removeClass("currentSize")}setCookie("fontSize",e,30),changeFont(e)});var i=getCookie("fontSize");""!=i&&changeFont(i),$("#font_selection_box").hasClass("19")?$("#fontsize_opt2").addClass("currentSize"):$("#font_selection_box").hasClass("22")?$("#fontsize_opt3").addClass("currentSize"):$("#fontsize_opt1").addClass("currentSize")});
