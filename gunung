<!-- refresh -->
<script>
    !(function () {
        var time = new Date().getTime();
    
        $(document.body).bind('mousemove keypress', function(e) {
            time = new Date().getTime();
        });
    
        function refresh() {
            if (new Date().getTime() - time >= 900000) {
                window.location.reload(true);
            } else {
                setTimeout(refresh, 10000);
            }
        }
        setTimeout(refresh, 10000);
    })();
</script>
<!-- jixie berita terkait -->
<script async>
    var thisPageURL = window.location.href;
    var keywordsDL = " apa itu gunung, gunung adalah, gunung, apa itu, Tribun Lampung, Tribunlampung.co.id";
    var titleDL = "Apa Itu Gunung";
   
    function createElement(tag, id, defaultClass, customClasses, iHTML) {
        var elm = document.createElement(tag);
        if (id) elm.id = id;
        if (defaultClass) elm.className = defaultClass;
        if (customClasses && customClasses.length > 0) {
            customClasses.forEach(function(cls) {
                elm.classList.add(cls);
            });
        }
        if (iHTML) elm.innerHTML = iHTML;
        return elm;
    }

    function createElementFromHTML(htmlString) {
        var div = document.createElement('div');
        div.innerHTML = htmlString.trim();
        return div.firstChild;
    }

    function fetchRecommendationsP(infoObj, jxUserInfo) {
      var url = null;
      if(infoObj.engine.type == 'jixie-api'){
          var s = '';
          ["accountid", "pageurl", "widget_id", "type", "keywords", "title", "algo", "customid", "count"].forEach(function(pname) {
              if (infoObj[pname])
                  s += (pname == "accountid" ? '?' : '&') + pname + '=' + encodeURIComponent(infoObj[pname]);
          });
          
          if (localStorage.getItem('_jxcht') !== null) {
              s += '&cohort=' + localStorage.getItem('_jxcht');
          }
          url = infoObj.engine.url + s;
      }

        return new Promise(function(resolve, reject){
            var xhr = new XMLHttpRequest();
            xhr.open(infoObj.engine['method'], url);
            xhr.onreadystatechange = function() {
                if (this.readyState == 4 && this.status >= 200 && this.status < 300) {
                    // trackEventGa('Tribun Jixie Article Recommendation', 'Content Request', 'algsrc='+infoObj.engine['tracker_algsrc']+',site=NEWS,device=mobile,status=success')
                    resolve(JSON.parse(xhr.response));
                } else if (this.readyState == 4 && this.status < 200 || this.status >= 300) {
                    // trackEventGa('Tribun Jixie Article Recommendation', 'Content Request', 'algsrc='+infoObj.engine['tracker_algsrc']+',site=NEWS,device=mobile,status=fail,response=' + this.status)
                    resolve(null);
                }
            }
            xhr.onerror = function() {
                resolve(null);
            }
            xhr.send();
        });
    }

    function fetchJSFileP(fileUrl) {
        return new Promise(function(resolve, reject) {
            var tag = document.createElement("script");
            var fst = document.getElementsByTagName("script")[0];
            fst.parentNode.insertBefore(tag, fst);
            tag.onload = function() {
                resolve();
            };
            tag.src = fileUrl;
        });
    };

    function handleClick(jxRecHelper0, url, pos) {
        jxRecHelper0.clicked(pos); //we need the position of the item in the widget
        // window.open(url); // open in new tab
        window.location = url; //open in current tab
        return false;
    }

    var cssClasses = {
        thumbnail_overlay_wrapper: 'arjW-img-overlay',
        thumbnail_wrapper: 'article_img',
        title: 'article_datas_title',
        container: 'arjW-list',
        wrapper: 'article_cards',
        wrapper_content: 'arjW-wrap',
        text_wrapper: 'article_datas'
    };

    function createDisplay(rand, container, resultObj, jxRecHelper) {
        return new Promise(function(resolve, reject) {
            var widgetWrapper = document.createElement('div');
            widgetWrapper.classList.add(cssClasses.container);
            container.appendChild(widgetWrapper);
            var widgetItemArr = [];
            try {
                var items = resultObj.items;
                if (items.length > 0) {
                    var counterNews = 0;
                    while (counterNews <= 8) {
                        items.some(function(item, index) {
                            counterNews++;
                            if (counterNews > 8) {
                                return true;
                            }
                            var divid = 'recItem-'+ rand + '-' + index;
                          
                            var objArr = {};
                            objArr['divid'] = divid;
                            objArr['id'] = jxRecHelper.jxUrlCleaner(item.url);
                            objArr['pos'] = index;
                            objArr['img'] = item.img;

                            if(resultObj.engine['type'] == 'jixie-api'){
                                objArr['algo'] = item.a; 
                            }
                            widgetItemArr.push(objArr);

                            var recItem = createElement('div', 'recItem-'+ rand + '-' + index , null, [cssClasses.wrapper]);
                            recItem.dataset.index = index;

                            var recItemWrapper = createElement('div', null, null, [cssClasses.wrapper_content]);

                            var imgWrapper = createElement('div', null, null, [cssClasses.thumbnail_wrapper]);
                            var imgElm = createElement('img', null, null, null);
                            imgElm.setAttribute('src', item.img);
                            imgElm.setAttribute('alt', item.title);
                            imgElm.setAttribute('width', 200);
                            imgElm.setAttribute('height', 110);

                            var imgOverlayWrapper = createElement('div', null, null, [cssClasses.thumbnail_overlay_wrapper]);
                            var imgOverlay = createElement('img', null, null, null);
                            imgOverlay.setAttribute('src', 'https://asset-1.tstatic.net/img/logo/tribun/svg/tribunnews.svg');
                            imgOverlay.setAttribute('alt', 'Tribunnews.com');
                            imgOverlay.setAttribute('loading', 'lazy');
                            imgOverlay.setAttribute('width', 74);
                            imgOverlay.setAttribute('height', 18);
                            imgOverlayWrapper.appendChild(imgOverlay);

                            imgWrapper.appendChild(imgElm);
                            imgWrapper.appendChild(imgOverlayWrapper);

                            var tracker_params = resultObj.engine['tracker_params'];
                            
                            // checking query params in item.url
                            var tempItemUrl = new URL(item.url)
                            if (tempItemUrl.search) {
                                tracker_params = tracker_params.replace('?', '&')
                            }

                            recItem.dataset.url = item.url;
                            var recItemWrapperA = createElement('a', null, null, null);
                            recItemWrapperA.setAttribute('href', item.url + tracker_params);
                            
                            var titleDiv = createElement('div', null, null, [cssClasses.title], item.title);

                            var textWrapper = createElement('div', null, null, [cssClasses.text_wrapper]);
                            textWrapper.appendChild(titleDiv);

                            recItemWrapper.appendChild(imgWrapper);
                            recItemWrapper.appendChild(textWrapper);

                            recItemWrapperA.appendChild(recItemWrapper);

                            recItem.appendChild(recItemWrapperA);

                            widgetWrapper.appendChild(recItem);
                            // hook up the click handler for this item
                            // recItem.onclick = handleClick.bind(null, jxRecHelper, item.url + tracker_params, index); 
                            recItemWrapperA.onclick = handleClick.bind(null, jxRecHelper, item.url + tracker_params, index);
                        });
                    }
                    var endDate = new Date();
                    var loadtime = (endDate.getTime() - startDate.getTime());
                    // trackEventGa('Tribun Jixie Article Recommendation', 'Content Loaded', 'algsrc='+resultObj.engine['tracker_algsrc']+',site=NEWS,device=mobile,loadtime=' + loadtime)
                    console.log(loadtime);
                    resolve();

                  
                    jxRecHelper.items(widgetItemArr);
                  
                    if(resultObj.engine['type'] == 'jixie-api'){
                        jxRecHelper.ready(resultObj.options.algo + ":" + resultObj.options.version);
                    }else{
                        jxRecHelper.ready(resultObj.engine['type']);
                    }

                } else {
                    jxRecHelper.error(204);
                    console.error("Error: no recommendation items");
                    return;
                }
            } catch (err) {
                jxRecHelper.error(901);
                console.log(err.stack);
            }
        });
    }
    var _jxRecSdkURL = 'https://scripts.jixie.media/jxrecsdk.1.0.min.js';
    var startDate = new Date();
    function OneWidget() {
            this._options = {
                accountid: '49294e929f73c07439de2f2801b98a58',
                pageurl: thisPageURL,
                widget_id: '1000013-dbXmIcdj8j',
                type: 'pages',
                container: 'artikel-rekomendasi-jixie',
                keywords: keywordsDL,
                title: titleDL,
                algo: 'contextual',
                customid: '1',
                count: 8
            };
            this._containerId = this._options.container;
            this._container = document.getElementById(this._containerId);
            this._engines = [
                {"url":"https://recommendation.jixie.media/v1/recommendation","method":"GET","pageurl":thisPageURL,"system":"jx","algo":"contextual","type":"jixie-api","tracker_params":"?source=widgetArtikelRekomendasi&engine=JXA","tracker_algsrc":"JXA"}
            ];
        OneWidget.prototype.kickOff = function() {
            startDate = new Date();
            var rand = Math.floor(Math.random() * 1000);
            var randSystem = Math.floor(Math.random() * 1);
            this._engine = this._engines[randSystem]
            this._options.system = this._engine['system']
            // trackEventGa('Tribun Jixie Article Recommendation', 'Widget Load', 'algsrc='+this._engine['tracker_algsrc']+',site=NEWS,device=mobile')
            var promJXSDK = fetchJSFileP(_jxRecSdkURL);
            var thisObj = this;

            var recHelperObj = null;
            var recResults = null;
            promJXSDK
                .then(function() {
                    recHelperObj = jxRecMgr.createJxRecHelper(thisObj._options);

                    var basicInfo = thisObj._options;
                    basicInfo.engine = thisObj._engine

                    var options = {
                        count: basicInfo['count'],
                        adpositions: "",
                        keywords: basicInfo['keywords'],
                        title: basicInfo['title'],
                        date_published: "2021-09-22 01:41:16",
                        pagecategory: "News",
                        algo: "contextual"
                    };

                    if (basicInfo.engine.type == 'jixie-api') {
                      return recHelperObj.getJxRecommendations(options);
                    } else {
                      return fetchRecommendationsP(basicInfo, recHelperObj.getJxUserInfo());
                    }
                })
                .then(function(resp) {
                    recResults = resp; 
                    recResults.engine = thisObj._engine;
                    if (!resp || !recResults.items || recResults.items.length == 0) {
                        recHelperObj.error(204);
                        throw "no recommendation results";
                    }
                })
                .then(function() {
                    document.getElementById("artikel-rekomendasi-jixie").classList.remove("-loading");
                    document.querySelector('.arjW-list').remove();
                    createDisplay(rand, thisObj._container, recResults, recHelperObj)
                        .then(function() {
                    })
                })
                .catch(function(error) {
                    console.log('Unable to create recommendations widget'+ error.stack + error.message);
                });
        }
    }

    var limit_gtm_ready = 1;

    function check_gtm_ready() {
        if ('ga' in window) {
            // create a new instance of our widget based on the options
            var newW = new OneWidget();
            newW.kickOff();
        } else {
            var timeout_gtm_ready = setTimeout(check_gtm_ready, 500)
            limit_gtm_ready += 1;
            if (limit_gtm_ready > 10) {
                clearTimeout(timeout_gtm_ready);
                var newW = new OneWidget();
                newW.kickOff();
            }
        }
    }
    check_gtm_ready()
</script>
<!-- end jixie berita terkait -->
</body>
</html>
