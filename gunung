	function moveScroller3() {
	    var move3 = function() {
	        var s3 = $("#wideskyscraper");
	        var st3 = $(window).scrollTop();
	        var ot3 = $("#fixads").offset().top - 105;
	        var bt3 = $("#fixsharebottom").offset().top - s3.height() - 120;
	
	        if(st3 > ot3) {
				var vtop = 65;
				if($(".main-menu").css("opacity") == "1") vtop = 65 + $(".main-menu").height();
	            s3.css({
	                position: "fixed",
	                top: vtop,
	
	            });
	        } 
	
			if(st3 > bt3 ) {
	            s3.css({
	                position: "absolute",
					top: "inherit",
					bottom	: "5px",
	            });
	        }
	
			if(st3 < ot3) {
	            s3.css({
	                position: "relative",
	                top: "0px",
	            });
	       	}
	
	    };
